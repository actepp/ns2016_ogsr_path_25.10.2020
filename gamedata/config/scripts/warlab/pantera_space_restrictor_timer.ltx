[logic]
active = sr_idle

[sr_idle]
on_info = {+pantera_fone_nayti_start} sr_timer1

[sr_timer1]
type = dec
argb = 255,255,255,255
start_value = 300000
on_value = 0 | %+pantera_fone_nayti_proval%
string = st_time_kvest_timer
on_info = {+pantera_fone_nayti_done} nil, {+pantera_fone_nayti_have} sr_timer2, {+pantera_fone_nayti_proval} sr_timer3

[sr_timer2]
type = dec
argb = 255,255,255,255
start_value = 0
on_info = {+pantera_fone_nayti_done} nil
string = st_time_kvest_done

[sr_timer3]
type = dec
argb = 255,255,255,255
start_value = 0
on_info = {+pantera_fone_nayti_done} nil
string = st_time_kvest_proval